def add(x,y):
    return x+y
def sub(x,y):
    return x-y
def div(x,y):
    return x/y
def multiply(x,y):
    return x*y
    
n1 = float(input('Enter first Number: '))
n2 = float(input('Enter second Number: '))
op = input('Choose Operation(+,-,/,*): ')


if op == '+':
    print(add(n1,n2))
elif op == '-':
    print(sub(n1,n2))
elif op == '/':
    print(div(n1,n2))
elif op == '*':
    print(multiply(n1,n2))
else:
    print('Invalid Input')
